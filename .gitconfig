[user]
	email = noelevans@gmail.com
	name = Noel Evans
[credential]
	helper = cache --timeout=172800

[color]
    ui = auto
[color "branch"]
    current = yellow reverse
    local = yellow
    remote = green
[color "diff"]
    meta = yellow bold
    frag = magenta bold
    old = red bold
    new = green bold
[color "status"]
    added = yellow
    changed = green
    untracked = cyan
[core]
    pager =
	editor = vim
[alias]
    st = status
    log = log --decorate --oneline --graph
